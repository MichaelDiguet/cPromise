{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "noEmitOnError": true,
    //"noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedParameters": false,
    "noUnusedLocals": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "sourceRoot": ".",
    "outDir": "dist",
    "skipLibCheck": true,
    "declaration": true
  },
  "include": [
    "src",
    "test"
  ],
  "exclude": [
  ]
}
